{
  "name": "grocery-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "local": "npm run build && nest start --debug --watch",
    "copyConfig": "cp src/config/* dist/config/",
    "prebuild": "rimraf dist",
    "build": "nest build && npm run copyConfig",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run build && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:build": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "migrate:generate": "DEBUG='*' prisma generate",
    "migrate:create": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "db:prototype": "prisma db push",
    "test:lint": "eslint --quiet src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier:check": "prettier --check src/**/*.ts",
    "prettier:write": "prettier --write src/**/*.ts",
    "prisma:format": "prisma format"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/throttler": "^3.1.0",
    "@prisma/client": "4.10.0",
    "amqp-connection-manager": "^4.1.4",
    "amqplib": "^0.10.2",
    "aws-sdk": "^2.1310.0",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "csvtojson": "2.0.10",
    "dayjs": "^1.11.9",
    "express-list-routes": "^1.1.3",
    "json-2-csv": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^2.2.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "nconf": "^0.12.0",
    "nest-winston": "^1.7.0",
    "ramda": "^0.28.0",
    "razorpay": "^2.8.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.4.0",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.13",
    "webpack": "^5.74.0",
    "winston": "^3.8.1",
    "winston-console-transport": "^1.1.9",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^33.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.1",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "prisma": "^4.6.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
